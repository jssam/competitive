
import java.util.Scanner;

public class binarytodecimal {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int N=in.nextInt();
		int multiplier=1;
		int ans=0;
		while(N!=0)
		{
			
			int rem=N%10;
			ans=ans+(rem*multiplier);
		 multiplier=multiplier*2;
		N =N/10;
		
		}System.out.print(ans);
		in.close();
	}

}
